<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<!-- 使用自动扫描包来定义别名，别名为类名 -->
		<package name="com.qst" />
	</typeAliases>
	<!-- 配置映射文件Mapper的位置 -->
	<mappers>
		<mapper resource="com/qst/mapper/UserDaoMapper.xml" />
		<mapper resource="com/qst/mapper/FirstDaoMapper.xml" />
		<mapper resource="com/qst/mapper/CourseDaoMapper.xml" />
	</mappers>
</configuration>


